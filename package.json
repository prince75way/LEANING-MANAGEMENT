{
  "name": "todolist",
  "version": "1.0.0",
  "description": "This is the task of the todolist",
  "scripts": {
    "local": "nodemon ",
    "start": " nodemon",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate -d src/services/databasetypeorm.ts",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- migration:run -d src/services/databasetypeorm.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/services/databasetypeorm.ts",
    "migration:show": "npm run typeorm -- migration:show -d src/services/databasetypeorm.ts"
  },
